{
    "name": "tachyon-drive-s3",
    "version": "0.2.0",
    "description": "Tachyon drive implementation for AWS S3",
    "main": "./dist/index.js",
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "nyc mocha",
        "coverage": "nyc report --reporter=lcovonly",
        "lint": "eslint src"
    },
    "files": [
        "dist"
    ],
    "repository": "github:mharj/tachyon-drive-s3",
    "keywords": [
        "storage",
        "driver",
        "nodejs"
    ],
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
            "spec",
            "mocha-junit-reporter"
        ]
    },
    "author": "mharj",
    "license": "MIT",
    "devDependencies": {
        "@types/chai": "^4.3.5",
        "@types/chai-as-promised": "^7.1.5",
        "@types/mocha": "^10.0.1",
        "@types/node": "^14.18.51",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "chai": "^4.3.7",
        "chai-as-promised": "^7.1.1",
        "dotenv": "^16.3.1",
        "eslint": "^8.43.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-deprecation": "^1.4.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-sonarjs": "^0.19.0",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "prettier": "^2.8.8",
        "tachyon-drive-node-fs": "^0.3.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3",
        "zod": "^3.21.4"
    },
    "dependencies": {
        "@avanio/logger-like": "^0.1.1",
        "@aws-sdk/client-s3": "^3.354.0",
        "@aws-sdk/types": "^3.347.0",
        "tachyon-drive": "^0.3.1"
    }
